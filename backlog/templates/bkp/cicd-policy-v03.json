{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "lambda:CreateFunction",
                "lambda:TagResource",
                "kms:UntagResource",
                "lambda:ListVersionsByFunction",
                "lambda:GetEventSourceMapping",
                "lambda:GetFunction",
                "lambda:GetFunctionConfiguration",
                "kms:ListResourceTags",
                "lambda:CreateEventSourceMapping",
                "lambda:UntagResource",
                "lambda:GetFunctionCodeSigningConfig",
                "kms:TagResource",
                "lambda:ListTags",
                "kms:*",
                "kms:CreateKey"
            ],
            "Resource": "*"
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": [
                "lambda:CreateFunction",
                "lambda:TagResource",
                "iam:ListRoleTags",
                "s3:GetObjectVersionTagging",
                "sqs:UntagQueue",
                "iam:CreateRole",
                "s3:CreateBucket",
                "lambda:GetFunctionConfiguration",
                "iam:PutRolePolicy",
                "dynamodb:DeleteTable",
                "dynamodb:DescribeTable",
                "iam:ListAttachedRolePolicies",
                "s3:PutLifecycleConfiguration",
                "dynamodb:DescribeContinuousBackups",
                "lambda:DeleteFunction",
                "iam:ListRolePolicies",
                "iam:GetRole",
                "lambda:GetEventSourceMapping",
                "s3:TagResource",
                "iam:DeleteRole",
                "sqs:GetQueueAttributes",
                "s3:DeleteBucketPolicy",
                "lambda:GetFunctionCodeSigningConfig",
                "dynamodb:CreateTable",
                "s3:PutBucketNotification",
                "sqs:DeleteQueue",
                "lambda:DeleteEventSourceMapping",
                "iam:GetRolePolicy",
                "kms:DeleteAlias",
                "s3:UntagResource",
                "kms:UntagResource",
                "iam:UntagRole",
                "lambda:ListVersionsByFunction",
                "iam:TagRole",
                "dynamodb:ListTagsOfResource",
                "kms:ListResourceTags",
                "sqs:ListQueueTags",
                "lambda:UntagResource",
                "dynamodb:UpdateContinuousBackups",
                "lambda:PutFunctionConcurrency",
                "s3:PutEncryptionConfiguration",
                "s3:ListTagsForResource",
                "iam:ListInstanceProfilesForRole",
                "iam:PassRole",
                "kms:TagResource",
                "dynamodb:TagResource",
                "s3:PutBucketTagging",
                "lambda:ListTags",
                "iam:DeleteRolePolicy",
                "kms:ScheduleKeyDeletion",
                "s3:GetObjectTagging",
                "kms:CreateGrant",
                "s3:PutBucketVersioning",
                "sqs:SetQueueAttributes",
                "kms:EnableKeyRotation",
                "s3:PutBucketPublicAccessBlock",
                "dynamodb:UntagResource",
                "lambda:GetFunction",
                "s3:PutBucketOwnershipControls",
                "s3:GetBucketVersioning",
                "dynamodb:DescribeTimeToLive",
                "sqs:TagQueue",
                "kms:CreateAlias",
                "sqs:CreateQueue"
            ],
            "Resource": [
                "arn:${local.partition}:iam::${ACCOUNT_ID}:role/*",
                "arn:${local.partition}:s3:::*",
                "arn:${local.partition}:sqs:*:${ACCOUNT_ID}:*",
                "arn:${local.partition}:dynamodb:*:${ACCOUNT_ID}:table/*",
                "arn:${local.partition}:kms:*:${ACCOUNT_ID}:alias/*",
                "arn:${local.partition}:kms:*:${ACCOUNT_ID}:key/*",
                "arn:${local.partition}:lambda:*:${ACCOUNT_ID}:function:*",
                "arn:${local.partition}:lambda:*:${ACCOUNT_ID}:event-source-mapping:*"
            ]
        },
        {
            "Sid": "VisualEditor2",
            "Effect": "Allow",
            "Action": [
                "s3:ListTagsForResource",
                "s3:PutObject",
                "s3:GetObjectAcl",
                "s3:GetObject",
                "s3:GetBucketTagging",
                "s3:UntagResource",
                "s3:TagResource"
            ],
            "Resource": "arn:${local.partition}:s3:::*/*"
        },
        {
            "Sid": "VisualEditor3",
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": "arn:${local.partition}:s3:::*"
        },
        {
            "Sid": "VisualEditor4",
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": "arn:${local.partition}:s3:::*/*"
        }
    ]
}