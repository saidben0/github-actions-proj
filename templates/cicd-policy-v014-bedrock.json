{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "kms:ListKeys",
                "kms:ListAliases",
                "kms:CreateKey",
                "kms:CreateAlias",
                "lambda:CreateFunction",
                "lambda:GetEventSourceMapping",
                "lambda:CreateEventSourceMapping"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "kms:GenerateDataKey",
                "kms:Decrypt",
                "kms:DescribeKey",
                "kms:UpdateKeyDescription",
                "kms:GetKeyPolicy",
                "kms:PutKeyPolicy",
                "kms:DeleteAlias",
                "kms:UpdateAlias",
                "kms:ListResourceTags",
                "kms:TagResource",
                "kms:UntagResource",
                "kms:ListResourceTags",
                "kms:ScheduleKeyDeletion",
                "kms:CancelKeyDeletion",
                "kms:CreateGrant",
                "kms:EnableKeyRotation",
                "kms:GetKeyRotationStatus"
            ],
            "Resource": [
                "arn:aws:kms:*:${ACCOUNT_ID}:alias/*",
                "arn:aws:kms:*:${ACCOUNT_ID}:key/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "lambda:GetFunction",
                "lambda:CreateFunction",
                "lambda:TagResource",
                "lambda:GetFunctionConfiguration",
                "lambda:DeleteFunction",
                "lambda:GetFunctionCodeSigningConfig",
                "lambda:PutFunctionConcurrency",
                "lambda:ListVersionsByFunction",
                "lambda:ListTags",
                "lambda:UntagResource",
                "lambda:AddPermission",
                "lambda:GetPolicy",
                "lambda:RemovePermission"
            ],
            "Resource": [
                "arn:aws:lambda:*:${ACCOUNT_ID}:function:*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "lambda:DeleteEventSourceMapping"
            ],
            "Resource": "arn:aws:lambda:*:${ACCOUNT_ID}:event-source-mapping:*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "dynamodb:DescribeTimeToLive",
                "dynamodb:DeleteTable",
                "dynamodb:DescribeTable",
                "dynamodb:CreateTable",
                "dynamodb:ListTagsOfResource",
                "dynamodb:UpdateContinuousBackups",
                "dynamodb:TagResource",
                "dynamodb:UntagResource",
                "dynamodb:DescribeContinuousBackups"
            ],
            "Resource": "arn:aws:dynamodb:*:${ACCOUNT_ID}:table/*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "iam:ListRoleTags",
                "iam:PutRolePolicy",
                "iam:ListAttachedRolePolicies",
                "iam:CreateRole",
                "iam:ListRolePolicies",
                "iam:GetRole",
                "iam:DeleteRole",
                "iam:UntagRole",
                "iam:TagRole",
                "iam:GetRolePolicy",
                "iam:ListInstanceProfilesForRole",
                "iam:PassRole",
                "iam:DeleteRolePolicy",
                "iam:GetPolicy",
                "iam:GetPolicyVersion",
                "iam:AttachRolePolicy",
                "iam:DetachRolePolicy",
                "iam:ListPolicies"
            ],
            "Resource": [ 
                "arn:aws:iam::${ACCOUNT_ID}:role/*",
                "arn:aws:iam::${ACCOUNT_ID}:*",
                "arn:aws:iam::aws:policy/service-role/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:PutObjectAcl",
                "s3:GetObjectTagging",
                "s3:GetBucketObjectLockConfiguration",
                "s3:DeleteObject",
                "s3:DeleteObjectVersion",
                "s3:PutObjectVersionAcl"
            ],
            "Resource": [
                "arn:aws:s3:::*",
                "arn:aws:s3:::*/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:CreateBucket",
                "s3:DeleteBucket",
                "s3:ListBucket",
                "s3:GetBucketLocation",
                "s3:GetBucketVersioning",
                "s3:GetBucketPolicy",
                "s3:PutBucketPolicy",
                "s3:DeleteBucketPolicy",
                "s3:GetBucketAcl",
                "s3:PutBucketAcl",
                "s3:GetBucketCors",
                "s3:PutBucketCors",
                "s3:PutBucketLogging",
                "s3:PutBucketNotification",
                "s3:PutBucketTagging",
                "s3:GetBucketWebsite",
                "s3:PutBucketWebsite",
                "s3:DeleteBucketWebsite",
                "s3:ListTagsForResource",
                "s3:GetBucketTagging",
                "s3:TagResource",
                "s3:UntagResource",
                "s3:GetAccelerateConfiguration",
                "s3:GetBucketRequestPayment",
                "s3:GetBucketLogging",
                "s3:GetLifecycleConfiguration",
                "s3:GetReplicationConfiguration",
                "s3:GetEncryptionConfiguration",
                "s3:PutEncryptionConfiguration",
                "s3:PutBucketVersioning",
                "s3:PutLifecycleConfiguration",
                "s3:GetBucketPublicAccessBlock",
                "s3:PutBucketPublicAccessBlock",
                "s3:GetBucketOwnershipControls",
                "s3:PutBucketOwnershipControls",
                "s3:ListBucketVersions",
                "s3:GetBucketNotification"
            ],
            "Resource": "arn:aws:s3:::*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "sqs:UntagQueue",
                "sqs:CreateQueue",
                "sqs:TagQueue",
                "sqs:GetQueueAttributes",
                "sqs:DeleteQueue",
                "sqs:SetQueueAttributes",
                "sqs:ListQueueTags"
            ],
            "Resource": "arn:aws:sqs:*:${ACCOUNT_ID}:*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "bedrock:GetAgent",
                "bedrock:DeleteAgent",
                "bedrock:CreateAgent",
                "bedrock:TagResource",
                "bedrock:UntagResource",
                "bedrock:ListTagsForResource",
                "bedrock:GetFoundationModel",
                "bedrock:PrepareAgent",
                "bedrock:CreateAgentActionGroup",
                "bedrock:DeleteAgentActionGroup",
                "bedrock:GetAgentActionGroup"
            ],
            "Resource": [
                "arn:aws:bedrock:us-east-1:${ACCOUNT_ID}:agent-alias/*",
                "arn:aws:bedrock:us-east-1:${ACCOUNT_ID}:agent/*",
                "arn:aws:bedrock:*::foundation-model/*"
            ]
        }
    ]
}
